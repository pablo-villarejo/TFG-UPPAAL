<nta>
	<declaration>
// Parámetros de tiempo de Metro
const int tParadaMax = 15;
const int tParadaMin = 10;
const int tMetroEnCruceMax = 10;
const int tMetroEnCruceMin = 5;
const int tMetroLejosMax = 90;
const int tMetroLejosMin = 50;

// Parámetros de tiempo de Coche
const int tCocheLLegaSemaforoMax = 30;
const int tCocheLLegaSemaforoMin = 15;
const int tCocheEnCruceMax = 15;
const int tCocheEnCruceMin = 5;
const int tCocheLejosMax = 90;
const int tCocheLejosMin = 50;

bool semaforoCoche[2]= {0,0} ; //0 -&gt; rojo 1 -&gt; verde
int cochesEnCruce = 0;
int metrosEnCruce = 0;  //Si el estado incial es "DentroCruce", hay que ponerlo a 2.
int metrosEnParada = 2; //Empieza a 2 si tenemos "enLaParada" como estado inicial

// Canal de sincronización para indicar paso permitido
broadcast chan greenCoche[2];
broadcast chan  redCoche [2];
broadcast chan metroAproximando; //No es necesario que sea urgent
broadcast chan metroSaliendo;    //No es necesario que sea urgent</declaration>
	<template>
		<name>Metro</name>
		<declaration>clock t;</declaration>
		<location id="id0" x="425" y="-8">
			<name x="434" y="-42">FueraCruce</name>
			<label kind="invariant" x="441" y="-17">t &lt;= tMetroLejosMax</label>
		</location>
		<location id="id1" x="-306" y="94">
			<name x="-391" y="42">DentroCruce</name>
			<label kind="invariant" x="-476" y="59">t &lt;= tMetroEnCruceMax</label>
		</location>
		<location id="id2" x="212" y="221">
			<name x="202" y="187">enLaParada</name>
			<label kind="invariant" x="161" y="238">t&lt;= tParadaMax</label>
		</location>
		<location id="id3" x="-161" y="264">
			<name x="-170" y="229">Alarma</name>
			<label kind="invariant" x="-170" y="212">t &lt;= 5</label>
		</location>
		<init ref="id2"/>
		<transition id="id4">
			<source ref="id3"/>
			<target ref="id3"/>
			<label kind="guard" x="-196" y="357">t == 5 &amp;&amp;
cochesEnCruce != 0</label>
			<label kind="assignment" x="-195" y="399">t := 0</label>
			<nail x="-111" y="357"/>
			<nail x="-213" y="357"/>
		</transition>
		<transition id="id5">
			<source ref="id3"/>
			<target ref="id1"/>
			<label kind="guard" x="-408" y="212">cochesEnCruce == 0 &amp;&amp;
t &gt;= 0</label>
			<label kind="assignment" x="-408" y="246">t:=0 , 
metrosEnCruce++,
metrosEnParada--</label>
		</transition>
		<transition id="id6">
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="guard" x="-60" y="255">cochesEnCruce != 0 &amp;&amp;
t &gt;= tParadaMax</label>
			<label kind="assignment" x="-60" y="297">t := 0</label>
		</transition>
		<transition id="id7">
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="guard" x="-8" y="68">cochesEnCruce == 0 &amp;&amp;
t &gt;= tParadaMin</label>
			<label kind="assignment" x="-8" y="110">t := 0 , 
metrosEnCruce++,
metrosEnParada--</label>
		</transition>
		<transition id="id8">
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="guard" x="441" y="110">t &gt;= tMetroLejosMin</label>
			<label kind="synchronisation" x="441" y="136">metroAproximando!</label>
			<label kind="assignment" x="441" y="161">t := 0,
metrosEnParada++</label>
			<nail x="416" y="204"/>
		</transition>
		<transition id="id9">
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="-144" y="-76">t &gt;= tMetroEnCruceMin</label>
			<label kind="synchronisation" x="-145" y="-59">metroSaliendo!</label>
			<label kind="assignment" x="-145" y="-42">t := 0, 
metrosEnCruce--</label>
			<nail x="-85" y="18"/>
		</transition>
	</template>
	<template>
		<name>SemaforoCoche</name>
		<parameter>int[0,1] d</parameter>
		<declaration>clock t;</declaration>
