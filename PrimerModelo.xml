			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="-144" y="-76">t &gt;= tEnCruceMin</label>
			<label kind="synchronisation" x="-145" y="-59">metroSaliendo!</label>
			<label kind="assignment" x="-145" y="-42">t := 0, 
metrosEnCruce--</label>
			<nail x="-85" y="18"/>
		</transition>
